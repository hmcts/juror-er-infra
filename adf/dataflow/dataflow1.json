{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "df_Column_Mapping",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "df_la_data_source",
						"type": "DatasetReference"
					},
					"name": "LAdataSource"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "lookup1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          FileName as string,",
				"          Col1Source as string,",
				"          Col1Target as string,",
				"          Col2Source as string,",
				"          Col2Target as string,",
				"          Col3Source as string,",
				"          Col3Target as string,",
				"          Col4Source as string,",
				"          Col4Target as string,",
				"          Col5Source as string,",
				"          Col5Target as string,",
				"          Col6Source as string,",
				"          Col6Target as string,",
				"          Col7Source as string,",
				"          Col7Target as string,",
				"          Col8Source as string,",
				"          Col8Target as string,",
				"          Col9Source as string,",
				"          Col9Target as string,",
				"          Col10Source as string,",
				"          Col10Target as string,",
				"          Col11Source as string,",
				"          Col11Target as string,",
				"          Col12Source as string,",
				"          Col12Target as string,",
				"          Col13Source as string,",
				"          Col13Target as string,",
				"          Col14Source as string,",
				"          Col14Target as string,",
				"          Col15Source as string,",
				"          Col15Target as string,",
				"          Col16Source as string,",
				"          Col16Target as string,",
				"          Col17Source as string,",
				"          Col17Target as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 101,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'name') ~> source1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> LAdataSource",
				"source1 lookup(FileName == ERROR_FUNCTION(''),",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1"
			]
		}
	}
}